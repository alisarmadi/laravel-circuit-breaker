{
    "name": "snapp-doctor/circuit-breaker",
    "autoload": {
        "psr-4": {
            "SnappDoctor\\CircuitBreaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SnappDoctor\\CircuitBreaker\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Ali Sarmadi"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SnappDoctor\\CircuitBreaker\\CircuitBreakerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
